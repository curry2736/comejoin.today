    <section class="job-form-section job-form-section--image">
      <div class="container">
        <div class="row">
          <div class="col-lg-8 mx-auto">
            <div class="job-form-box">
              <h2 class="heading">Find an <span class="accent">opportunity</span> for <span class="accent">you</span>.</h2>
              <form id="job-main-form" method="GET" action="/search/query" class="job-main-form">
                <div class="controls">
                  <div class="row align-items-center">
                    <div class="col-md-5">
                      <div class="form-group">
                        <label for="name">Opportunity</label>
                        <input type="text" id="name" name="name" placeholder="Enter a name" class="form-control">
                      </div>
                    </div>
                    <div class="col-md-5">
                      <div class="form-group">
                        <label for="location">Location</label>
                        <input type="text" id="location" name="location" placeholder="Enter a location" class="form-control">
                      </div>
                    </div>
                    <div class="col-md-2">
                      <button type="submit" name="submit" class="btn btn-outline-white-primary job-main-form__button"><i class="fa fa-search"></i></button>
                    </div>
                  </div>
                </div>
                <div class="controls">
                  <div class="row align-items-end">
                    <div class="col-sm-2 col-lg-2"></div>
                    <div class="col-sm-3 col-lg-3">
                      <label for="internshipCheck">Internship</label>
                      <input type="checkbox" id="internshipCheck" name="internshipCheck" checked>
                    </div>
                    <div class="col-sm-3 col-lg-3">
                      <label for="volunteeringCheck">Club</label>
                      <input type="checkbox" id="volunteeringCheck" name="volunteeringCheck" checked>
                    </div>
                    <div class="col-sm-3 col-lg-3">
                      <label for="workshopCheck">Workshop</label>
                      <input type="checkbox" id="workshopCheck" name="workshopCheck" checked>
                    </div>
                    <div class="col-sm-1 col-lg-1"></div>
                  </div>
                </div>
              </form>
            </div>
          </div>
        </div>
      </div>
    </section>

    <section>
      <div class="container">
        <h4 class="heading">Most Recent Internships</h4>
        <% recents.internships.forEach( value => { %>
          <div class="job-listing">
            <div class="row">
              <div class="col-md-12 col-lg-6">
                <div class="row">
                  <div class="col-2"><img src="../../../d19m59y37dris4.cloudfront.net/jobs/2-1-0/img/company-3.png" alt="Bootstrapious " class="img-fluid"></div>
                  <div class="col-10">
                    <h4 class="job__title"><a href="/details/<%= value.id %>"><%= value.jobTitle %></a></h4>
                    <p class="job__company">
                      <%= value.companyName %>
                    </p>
                  </div>
                </div>
              </div>
              <div class="col-10 col-md-3 col-lg-2 ml-auto"><i class="fa fa-map-marker job__location"></i><%=value.city%>, <%=value.state%></div>
              <div class="col-10 col-md-3 col-lg-3 ml-auto">
                <% var daysSincePost = Math.round((Date.now() - value.datePosted.getTime()) / (1000 * 60 * 60 * 24))%>
                Posted <%= daysSincePost%> days ago
              </div>
              <% if (user != null) { %>
                <% if (user.favorites.includes(value.id)) {%>
                  <form action="/favorite" method="POST" target="hiddenFrame">
                    <div class="col-sm-12 col-md-2 col-lg-1">
    
                      <input type="hidden" name="_id" value= <%= value._id %> />
    
                      <div class="job__star">
                        <button onclick="favorite(this);" data-toggle="tooltip" data-placement="top" title="Save to favorites" class="job__star__link">
                          <i class="fa fa-star favorited-star"></i>
                        </button>
                      </div>
                    </div>
                  </form>
                <% } else { %>
                  <form action="/favorite" method="POST" target="hiddenFrame">
                    <div class="col-sm-12 col-md-2 col-lg-1">
    
                      <input type="hidden" name="_id" value= <%= value._id %> />
    
