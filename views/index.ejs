                        <button onclick="favorite(this);" data-toggle="tooltip" data-placement="top" title="Save to favorites" class="job__star__link">
                          <i class="fa fa-star"></i>
                        </button>
                      </div>
                    </div>
                  </form>
                <% } %>
              <% } else {%>
                <form action="/signup">
                  <div class="col-sm-12 col-md-2 col-lg-1">
  
                    <input type="hidden" name="_id" value= <%= value._id %> />
  
