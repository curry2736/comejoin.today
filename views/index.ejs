                      <button type="submit" data-toggle="tooltip" data-placement="top" title="Login to save to favorites" class="job__star__link">
                        <i class="fa fa-star"></i>
                      </button>
                    </div>
                  </div>
                </form>
              <% } %>
            </div>
          </div>
        <% }) %>
