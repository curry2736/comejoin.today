    <section class="admin-section">
      <h3 class="margin-bottom "style="text-align:center;">Pending Users</h3>
      <p style="text-align:center;">By clicking "approve" you will give the user permissions to create listings for school clubs</p>
      <div class="container">
        
        <div class="row-admin-dashboard">
          
          <div class="col-lg-12">
            <div class="table-responsive">
              <table class="table table-striped table-hover table-client-dashboard"> 
                <thead>
                  <tr>
                    <th>Email</th>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>School</th>
                    <th>Actions</th>
                  </tr>
                </thead>
                <tbody>
                  
                  <% pendingUsers.forEach(pendingUser => { %>
                    <tr>
                      <td><%= pendingUser.email %>
                      </td>
                      <td><%=pendingUser.firstName%></td>
                      </td>
                      <td><%=pendingUser.lastName%></td>
                      </td>
                      <td><%=pendingUser.school%></td>
                      </td>
                      <td>
                        <form type="hidden"  method="POST" action="/admin-dashboard/approve/<%=pendingUser._id%>?_method=PUT">
                          <input name="userId" value=<%= pendingUser._id%> type="hidden">
                          <button type="submit"class="btn btn-outline-white-secondary"><i class="fa fa-check"></i>Approve</button>
                        </form>
                        <form type="hidden"  method="POST" action="/admin-dashboard/disapprove/<%=pendingUser._id%>?_method=PUT">
                          <input name="userId" value=<%= pendingUser._id%> type="hidden">
                          <button type="submit"class="btn btn-outline-white-secondary"><i class="fa fa-times-circle-o"></i>Disapprove</button>
                        </form>
                        <!--<i class="fa fa-check-circle-o"></i><span class="hidden-xs hidden-sm">Mark as </span>filled</a><br><a href="#" class="btn btn-outline-white-secondary">-->
                      </td>
                    </tr>
                  <% }) %>
                </tbody>
              </table>
            </div>
           </div>

      </div>
    </section>
    <section class="admin-section">
      <h3 class="margin-bottom "style="text-align:center;">Approved Users</h3>
      <div class="container">
        
        <div class="row-admin-dashboard">
          
          <div class="col-lg-12 ">
            <div class="table-responsive">
              <table class="table table-striped table-hover table-client-dashboard"> 
                <thead>
                  <tr>
                    <th>Email</th>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>School</th>
    
                    <th>Actions</th>
                  </tr>
                </thead>
                <tbody>
                  
                  <% allowedUsers.forEach(allowedUser => { %>
                    <tr>
                      <td><%= allowedUser.email %>
                      </td>
                      <td><%=allowedUser.firstName%></td>
                      </td>
                      <td><%=allowedUser.lastName%></td>
                      </td>
                      <td><%=allowedUser.school%></td>
                      
                      <td>
                        <form type="hidden"  method="POST" action="/admin-dashboard/disapprove/<%=allowedUser._id%>?_method=PUT">
                          <input name="userId" value=<%= allowedUser._id%> type="hidden">
                          <button type="submit"class="btn btn-outline-white-secondary"><i class="fa fa-times-circle-o"></i>Remove permissions </button>
                        </form>
                        <!--<i class="fa fa-check-circle-o"></i><span class="hidden-xs hidden-sm">Mark as </span>filled</a><br><a href="#" class="btn btn-outline-white-secondary">-->
                      </td>
                    </tr>
                  <% }) %>
                </tbody>
              </table>
            </div>
           </div>
