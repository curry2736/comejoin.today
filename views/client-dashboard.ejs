    <section>
      <div class="container">
        <div class="row-client-dashboard">
          <div class="col-lg-12 text-right mb-5"><a href="/company" class="btn btn-outline-white-primary"> <i class="fa fa-plus"></i>Add new position</a></div>
          <div class="col-lg-12">
            <div class="table-responsive">
              <table class="table table-striped table-hover table-client-dashboard"> 
                <thead>
                  <tr>
                    <th>Event title</th>
                    <th>Event type</th>
                    <th>Location</th>
                    <!--<th>Position filled</th>-->
                    <th>Date posted</th>
                    <th>Date expiring</th>
                    <!--<th>Applicants</th>-->
                    <th>Actions</th>
                  </tr>
                </thead>
                <tbody>
                  <% events.internships.forEach(event => { %>
                    <tr>
                      <th><a href="/details/<%= event._id %>"><%= event.jobTitle %></a>
                      
                      </th>
                      <td>Internship</td>
                      <td><%= event.city %>, <%= event.state %></td>
                      <!--<td> <span class="badge badge-success">Filled</span>-->
                      <td><%= event.datePosted.toLocaleDateString('en-US') %></td>
                      <td><%= event.dateExpiring.toLocaleDateString('en-US') %></td>
                      <!--<td> <a href="client-applicants.html" class="btn btn-success"> <i class="fa fa-users"></i>4 <span class="hidden-xs hidden-sm">Applicants</span></a>-->
                      </td>
                      <td> 
                        <!--<a href="client-job.html" class="btn btn-outline-white-secondary"> <i class="fa fa-edit"></i>Edit</a><br>-->
                        <form type="hidden"  method="POST" action="/client-dashboard/internships/<%= event._id %>?_method=DELETE">
                          <input name="userId" value=<%= user._id%> type="hidden">
                          <button type="submit"class="btn btn-outline-white-secondary"><i class="fa fa-times-circle-o"></i>Delete</button>
                        </form>
                        <!--<i class="fa fa-check-circle-o"></i><span class="hidden-xs hidden-sm">Mark as </span>filled</a><br><a href="#" class="btn btn-outline-white-secondary">-->
                      </td>
                    </tr>
                  <% }) %>
                  <% events.volunteerings.forEach(event => { %>
                    <tr>
                      <th><a href="/details/<%= event._id %>"><%= event.eventName %></a>
                      </th>
                      <td>Volunteering</td>
                      <td><%= event.city %>, <%= event.state %></td>
                      <!--<td> <span class="badge badge-success">Filled</span>-->
                      <td><%= event.datePosted.toLocaleDateString('en-US') %></td>
                      <td><%= event.dateExpiring.toLocaleDateString('en-US') %></td>
                      <!--<td> <a href="client-applicants.html" class="btn btn-success"> <i class="fa fa-users"></i>4 <span class="hidden-xs hidden-sm">Applicants</span></a>-->
                      </td>
                      <td> 
                        <!--<a href="client-job.html" class="btn btn-outline-white-secondary"> <i class="fa fa-edit"></i>Edit</a><br>-->
                        <form type="hidden" value=<%= user._id%> method="POST" action="/client-dashboard/volunteerings/<%= event._id %>?_method=DELETE">
                          <button class="btn btn-outline-white-secondary"><i class="fa fa-times-circle-o"></i>Delete</button>
                        </form>
                        <!--<i class="fa fa-check-circle-o"></i><span class="hidden-xs hidden-sm">Mark as </span>filled</a><br><a href="#" class="btn btn-outline-white-secondary">-->
                      </td>
                    </tr>
                  <% }) %>
                  <% events.workshops.forEach(event => { %>
                    <tr>
                      <th><a href="/details/<%= event._id %>"><%= event.eventName %></a>
                      </th>
                      <td>Workshop</td>
                      <td><%= event.city %>, <%= event.state %></td>
                      <!--<td> <span class="badge badge-success">Filled</span>-->
                      <td><%= event.datePosted.toLocaleDateString('en-US') %></td>
                      <td><%= event.dateExpiring.toLocaleDateString('en-US') %></td>
                      <!--<td> <a href="client-applicants.html" class="btn btn-success"> <i class="fa fa-users"></i>4 <span class="hidden-xs hidden-sm">Applicants</span></a>-->
                      </td>
                      <td> 
                        <!--<a href="client-job.html" class="btn btn-outline-white-secondary"> <i class="fa fa-edit"></i>Edit</a><br>-->
                        <form type="hidden" value=<%= user._id%> method="POST" action="/client-dashboard/workshops/<%= event._id %>?_method=DELETE">
                          <button class="btn btn-outline-white-secondary"><i class="fa fa-times-circle-o"></i>Delete</button>
                        </form>
                        <!--<i class="fa fa-check-circle-o"></i><span class="hidden-xs hidden-sm">Mark as </span>filled</a><br><a href="#" class="btn btn-outline-white-secondary">-->
                      </td>
                    </tr>
                  <% }) %>
                </tbody>
              </table>
            </div>
