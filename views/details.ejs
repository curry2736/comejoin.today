            <% if (details.type === "Volunteering"){%>
              <h4>About <%= details.org%></h4>
              <p class="job-detail__company-description"><h5>Contact Information</h5><br>
                <ul>
                  <li><b>Advisor Email:</b> <%= details.result.advisorEmail%></li>
                  <li><b>President Email:</b><%= details.result.presidentEmail%></li>
                </ul></p>
              <p class="job-detail__social social social--outline">
                <a href= "<%= details.result.websiteLink%>" target="_blank" data-toggle="tooltip" data-placement="top" title="Website" class="link" > <i class="fa fa-link"></i> </a>
                <a href= "<%= details.result.teamsLink%>" target="_blank" data-toggle="tooltip" data-placement="top" title="Website"> <i class="fa fa-windows"></i> </a> 
              </p>
              <div class="job-detail__apply-top"><a href=<%= details.result.formLink%> target="_blank" class="btn btn-outline-white-primary">Apply for this position</a></div>   

            <%} else{ %>
