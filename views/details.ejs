            <p>Posted: <%= details.dates.datePosted%></p>
            <h4><%= details.name %></h4>       
            <ol>
              <li>Start Date: <%= details.dates.startDate%></li>
              <li>End Date: <%= details.dates.endDate%></li>
            </ol>
            <h5>Description</h5>
            <blockquote class="blockquote">
              <p id="detailDesc"><%= details.result.description%></p>
              <script>
                var text=document.getElementById('detailDesc').innerHTML.split('\n');
                var updatedText = text.join('<br />');
                document.getElementById('detailDesc').innerHTML = updatedText
              </script>
            </blockquote>
            <div style="padding-bottom: 56.25%; position: relative;">
              <iframe style="position:absolute;" width="100%" height="100%" frameborder="0" allow="encrypted-media; gyroscope;" allowfullscreen src="https://www.youtube.com/embed/<%=details.result.youtubeLink.split('watch?v=')[1]%>"></iframe>
            </div>
